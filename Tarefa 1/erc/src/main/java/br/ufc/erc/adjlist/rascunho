

        for (CtType<?> ctType : model.getAllTypes()) {
            String typeName = ctType.getQualifiedName();
            Set<String> referencedTypes = new HashSet<>();


            for (CtType<?> referencedType : ctType.getElements(new TypeFilter<>(CtType.class))) {
                if (!referencedType.getQualifiedName().equals(typeName)) {
                    referencedTypes.add(referencedType.getQualifiedName());
                }
            }


            adjacencyList.put(typeName, referencedTypes);
        }


        for (Map.Entry<String, Set<String>> entry : adjacencyList.entrySet()) {
            System.out.println("Type: " + entry.getKey());
            System.out.println("Referenced Types: " + entry.getValue());
            System.out.println();
        }